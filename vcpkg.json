{
  "name": "app",
  "version": "1.0",
  "dependencies": [
    "cppwinrt",
    "d3dx12",
    "directx-headers",
    "directx12-agility",
    {
      "name": "directxmath",
      "features": [
        "dx12",
        "xdsp"
      ]
    },
    {
      "name": "directxmesh",
      "features": [
        "dx12",
        "spectre",
        "tools"
      ]
    },
    {
      "name": "directxtex",
      "features": [
        "dx12",
        "spectre",
        "tools"
      ]
    },
    {
      "name": "directxtk12",
      "features": [
        "gameinput",
        "spectre",
        "tools",
        "xaudio2-9",
        "xaudio2redist",
        "xinput"
      ]
    },
    "fmt",
    {
      "name": "freetype",
      "features": [
        "brotli",
        "bzip2",
        "error-strings",
        "png",
        "subpixel-rendering",
        "zlib"
      ]
    },
    "reflectcpp",
    "termcolor"
  ]
